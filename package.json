{
  "name": "webpack-npm-package-template",
  "version": "1.1.0",
  "description": "Framework neutral template for npm packages built with Webpack and Babel",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && webpack --config webpack.build.config.js",
    "build-release": "npm run clean && webpack -p --config webpack.build.config.js",
    "watch": "webpack --config webpack.build.config.js --watch",
    "start": "webpack-dev-server --config webpack.serve.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aeinbu/npm-package-for-ng-template.git"
  },
  "author": "Arjan Einbu <arjan@einbu.no> (http://blog.einbu.no)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "css-loader": "^0.28.4",
    "deepmerge": "^1.5.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.3",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0"
  }
}
