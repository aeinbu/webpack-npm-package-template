{
  "name": "webpack-npm-package-template",
  "version": "1.1.0",
  "description": "Framework neutral template for npm packages built with Webpack and Babel",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && webpack",
    "watch": "webpack --config webpack.build.config.js --watch",
    "start": "webpack-dev-server --config webpack.serve.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aeinbu/npm-package-for-ng-template.git"
  },
  "author": "Arjan Einbu <arjan@einbu.no> (http://blog.einbu.no)",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^1.0.0",
    "deepmerge": "^2.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.1",
    "rimraf": "^2.6.2",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "font-awesome": "^4.7.0"
  }
}
